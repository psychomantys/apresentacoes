\ProvidesPackageRCS $Header: /cvsroot/latex-beamer/latex-beamer/themes/color/beamercolorthemewhale.sty,v 1.4 2007/01/28 20:48:24 tantau Exp $


\mode<presentation>

\definecolor{lccvbluea}{RGB}{46,83,121}
\definecolor{lccvblueb}{RGB}{0,255,0}
\definecolor{lccvbluec}{RGB}{0,0,255}
\definecolor{lccvblued}{RGB}{54,98,141}

\setbeamercolor*{normal text}{fg=black,bg=white}
%\setbeamercolor*{alerted text}{fg=black}
\setbeamercolor*{example text}{fg=black}
\setbeamercolor*{structure}{fg=black}


\setbeamercolor*{palette primary}{use=structure,fg=white,bg=lccvbluea}
\setbeamercolor*{palette secondary}{use=structure,fg=white,bg=lccvblueb}
\setbeamercolor*{palette tertiary}{use=structure,fg=white,bg=lccvbluec}
\setbeamercolor*{palette quaternary}{fg=white,bg=lccvblued}

\setbeamercolor*{sidebar}{use=structure,bg=structure.fg}
  
\setbeamercolor*{palette sidebar primary}{use=structure,fg=structure.fg!10,bg=lccvbluea}
\setbeamercolor*{palette sidebar secondary}{fg=white,bg=lccvblueb}
\setbeamercolor*{palette sidebar tertiary}{use=structure,fg=structure.fg!50,bg=lccvbluec}
\setbeamercolor*{palette sidebar quaternary}{fg=white,bg=lccvblued}

\setbeamercolor{item projected}{fg=black,bg=black!20}
\setbeamercolor{item projected}{fg=white,bg=lccvblued}
\setbeamercolor{description item}{use=structure, fg=lccvblued}

\setbeamercolor*{titlelike}{parent=palette primary}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\mode
<all>
